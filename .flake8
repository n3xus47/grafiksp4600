[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    build,
    dist,
    *.egg-info,
    .eggs,
    .tox,
    .pytest_cache,
    htmlcov,
    .coverage,
    .mypy_cache
per-file-ignores =
    __init__.py:F401
    tests/*:S101,S105,S106,S108
max-complexity = 10
